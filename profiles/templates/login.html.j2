{% extends 'auth.html.j2' %}
{% block 'title'%}Log In{% endblock %} 
{% block 'section-title'%} Log In {% endblock %}
{% block 'section-title-description'%}
  Hello there, Log In
{% endblock %}
{% block 'content' %}

  <form method="POST" action="{% url 'login' %}">
    {% csrf_token %}

    {{ form }}

    <div class="text-center">
      <input class="button" type="submit" value="Log In"/>
      <a href="{% url 'register'%}"><div class="button">Create an account</div></a>
    </div>
  </form>
{% endblock %}